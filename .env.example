# Frida Orchestrator - Environment Variables
# Copy this file to .env and fill in your keys

# Google Gemini API Key (REQUIRED)
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration (Optional - for storage/audit)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_BUCKET=processed-images

# Supabase JWT Authentication
# Get JWT secret from: Supabase Dashboard > Settings > API > JWT Secret
SUPABASE_JWT_SECRET=your_supabase_jwt_secret_here

# Authentication Toggle
# Set to "true" to enable JWT validation (production)
# Set to "false" to disable auth (development)
AUTH_ENABLED=false

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=true
